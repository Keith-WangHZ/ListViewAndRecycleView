<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item android:id="@android:id/background">
	<shape>
		<corners android:radius="10dip" />
		<gradient android:startColor="#26ffffff"
		android:centerColor="#26ffffff" android:endColor="#26ffffff" android:angle="270" />
	</shape>
</item>
 
<item android:id="@android:id/progress">
<clip>
	<shape>
		<corners android:radius="10dip" />
		<gradient android:startColor="#ff0c7b00"
		android:endColor="#ff7eff00"
		android:angle="0" />
	</shape>
</clip>
</item>
</layer-list>  
